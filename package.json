{
    "name": "the-plug",
    "version": "3.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
        "dev:local": "env-cmd -f ./.env.local npm run dev",
        "dev:pre": "env-cmd -f ./.env.pre npm run dev",
        "dev:prod": "env-cmd -f ./.env.prod npm run dev",
        "start": "node ./dist/index.js",
        "build": "tsc"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "body-parser": "^1.18.3",
        "cors": "^2.8.5",
        "date-fns": "^2.21.1",
        "env-cmd": "^10.1.0",
        "express": "^4.16.4",
        "mongoose": "^5.12.1"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.0",
        "@types/cors": "^2.8.10",
        "@types/date-fns": "^2.6.0",
        "@types/express": "^4.17.11",
        "@types/mongoose": "^5.10.4",
        "@types/node": "^14.14.35",
        "dotenv": "^8.2.0",
        "ts-node": "^9.1.1",
        "ts-node-dev": "^1.1.6",
        "typescript": "^4.2.3"
    }
}
